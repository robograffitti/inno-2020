<?xml version="1.0" ?>
<robot name="tb3_inno" xmlns:xacro="http://ros.org/wiki/xacro">
  <xacro:include filename="$(find tb3_inno_description)/urdf/common_properties.xacro"/>
  <!-- <xacro:include filename="$(find tb3_inno_description)/urdf/tb3_inno.gazebo.xacro"/> -->

  <link name="base_footprint"/>

  <joint name="base_joint" type="fixed">
    <parent link="base_footprint"/>
    <child link="base_link"/>
    <origin xyz="0.0 0.0 0.009742" rpy="0 0 0"/>
  </joint>

  <link name="base_link">
    <visual>
      <!-- <origin xyz="-0.032 0 0.0" rpy="0 0 0"/> -->
      <origin xyz="0.0 0.0 0.0" rpy="0 0 0"/>
      <geometry>
        <mesh filename="package://tb3_inno_description/meshes/inno_base.stl" scale="0.001 0.001 0.001"/>
      </geometry>
      <material name="light_black"/>
    </visual>

    <collision>
      <!-- <origin xyz="-0.032 0 0.070" rpy="0 0 0"/> -->
      <origin xyz="0.0 0.0 0.0" rpy="0 0 0"/>
      <geometry>
        <box size="0.140 0.140 0.143"/>
      </geometry>
    </collision>

    <inertial>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <mass value="3.940e-01"/>
      <inertia ixx="1.000e-03" ixy="2.165e-06" ixz="2.305e-06"
               iyy="2.000e-03" iyz="-1.243e-06"
               izz="1.000e-03" />
    </inertial>
  </link>

  <!-- left -->
  <joint name="wheel_left_front_joint" type="continuous">
    <parent link="base_link"/>
    <child link="wheel_left_front_link"/>
    <origin xyz="0.064 0.071 0.023258" rpy="0 0 0"/>
    <axis xyz="0 1 0"/>
  </joint>

  <link name="wheel_left_front_link">
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <mesh filename="package://tb3_inno_description/meshes/inno_tire_left.stl" scale="0.001 0.001 0.001"/>
      </geometry>
      <material name="dark"/>
    </visual>

    <collision>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <cylinder length="0.018" radius="0.033"/>
      </geometry>
    </collision>

    <!-- inertia should be modified. this is tmp values from original copy.-->
    <inertial>
      <origin xyz="0 0 0" />
      <mass value="2.8498940e-02" />
      <inertia ixx="1.1175580e-05" ixy="-4.2369783e-11" ixz="-5.9381719e-09"
               iyy="1.1192413e-05" iyz="-1.4400107e-11"
               izz="2.0712558e-05" />
      </inertial>
  </link>

  <joint name="wheel_left_back_joint" type="continuous">
    <parent link="base_link"/>
    <child link="wheel_left_back_link"/>
    <origin xyz="-0.064 0.071 0.023258" rpy="0 0 0"/>
    <axis xyz="0 1 0"/>
  </joint>

  <link name="wheel_left_back_link">
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <mesh filename="package://tb3_inno_description/meshes/inno_tire_left.stl" scale="0.001 0.001 0.001"/>
      </geometry>
      <material name="dark"/>
    </visual>

    <collision>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <cylinder length="0.018" radius="0.033"/>
      </geometry>
    </collision>

    <!-- inertia should be modified. this is tmp values from original copy.-->
    <inertial>
      <origin xyz="0 0 0" />
      <mass value="2.8498940e-02" />
      <inertia ixx="1.1175580e-05" ixy="-4.2369783e-11" ixz="-5.9381719e-09"
               iyy="1.1192413e-05" iyz="-1.4400107e-11"
               izz="2.0712558e-05" />
      </inertial>
  </link>


  <!-- right -->
  <joint name="wheel_right_front_joint" type="continuous">
    <parent link="base_link"/>
    <child link="wheel_right_front_link"/>
    <origin xyz="0.064 -0.071 0.023258" rpy="0 0 0"/>
    <axis xyz="0 1 0"/>
  </joint>

  <link name="wheel_right_front_link">
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <mesh filename="package://tb3_inno_description/meshes/inno_tire_right.stl" scale="0.001 0.001 0.001"/>
      </geometry>
      <material name="dark"/>
    </visual>

    <collision>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <cylinder length="0.018" radius="0.033"/>
      </geometry>
    </collision>

    <!-- inertia should be modified. this is tmp values from original copy.-->
    <inertial>
      <origin xyz="0 0 0" />
      <mass value="2.8498940e-02" />
      <inertia ixx="1.1175580e-05" ixy="-4.2369783e-11" ixz="-5.9381719e-09"
               iyy="1.1192413e-05" iyz="-1.4400107e-11"
               izz="2.0712558e-05" />
    </inertial>
  </link>

  <joint name="wheel_right_back_joint" type="continuous">
    <parent link="base_link"/>
    <child link="wheel_right_back_link"/>
    <origin xyz="-0.064 -0.071 0.023258" rpy="0 0 0"/>
    <axis xyz="0 1 0"/>
  </joint>

  <link name="wheel_right_back_link">
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <mesh filename="package://tb3_inno_description/meshes/inno_tire_right.stl" scale="0.001 0.001 0.001"/>
      </geometry>
      <material name="dark"/>
    </visual>

    <collision>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <cylinder length="0.018" radius="0.033"/>
      </geometry>
    </collision>

    <!-- inertia should be modified. this is tmp values from original copy.-->
    <inertial>
      <origin xyz="0 0 0" />
      <mass value="2.8498940e-02" />
      <inertia ixx="1.1175580e-05" ixy="-4.2369783e-11" ixz="-5.9381719e-09"
               iyy="1.1192413e-05" iyz="-1.4400107e-11"
               izz="2.0712558e-05" />
      </inertial>
  </link>

