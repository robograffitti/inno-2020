<?xml version="1.0"?>
<!-- Open_Manipulator Chain -->
<robot name="omx_inno" xmlns:xacro="http://ros.org/wiki/xacro">

  <!-- Include tb3_inno URDF -->
  <xacro:include filename="$(find tb3_inno_description)/urdf/tb3_inno.urdf.xacro" />

  <!-- Include OpenManipulator Chain Gazebo Parameter -->
